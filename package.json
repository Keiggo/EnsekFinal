{
  "name": "mica-4-e2e",
  "version": "0.0.1",
  "description": "End to end tests for MICA 4",
  "@comment scripts": {
    "build:check": [
      "Checks that the project builds.",
      "Use CTRL-SHIFT-P > Tasks: Configure Default Build and select npm: build:check to set this as the default build task.",
      "Set problemMatcher to '$tsc'",
      "CTRL-SHIFT-B will then check the entire project and highlight any issues in the PROBLEMS pane."
    ]
  },
  "scripts": {
    "test": "jest --group=regression --runInBand --detectOpenHandles --setupFiles ./environments/test.env",
    "test:smoke": "jest --group=smoke-test --runInBand --detectOpenHandles --setupFiles ./environments/test.env",
    "test:local:regression": "jest --group=regression --runInBand --verbose --detectOpenHandles --setupFiles ./environments/local.env",
    "test:local:smoke": "jest --group=smoke-test --runInBand --verbose --detectOpenHandles --setupFiles ./environments/local.env",
    "test:local:ui": "jest --group=ui --runInBand --verbose --detectOpenHandles --setupFiles ./environments/local.env",
    "test:local:api": "jest --group=api --runInBand --verbose --detectOpenHandles --setupFiles ./environments/local.env",
    "lint:fix": "eslint --fix **/*.ts",
    "build:check": "npm run lint:fix && tsc --noEmit",
    "refreshVSToken": "vsts-npm-auth -config .npmrc"
  },
  "keywords": [
    "jest"
  ],
  "author": {
    "name": "Telent"
  },
  "private": true,
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@jest/types": "^28.1.1",
    "@types/jest": "^26.0.15",
    "@types/luxon": "ts4.3",
    "@types/puppeteer": "^5.4.0",
    "digest-fetch": "^1.1.6",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "guid-typescript": "^1.0.9",
    "jest": "^26.6.3",
    "jest-docblock": "^27.4.0",
    "jest-html-reporter": "^3.4.2",
    "jest-html-reporters": "^3.0.5",
    "jest-junit": "^12.2.0",
    "jest-preset-angular": "^8.4.0",
    "jest-runner": "^26.6.3",
    "jest-runner-groups": "^2.1.0",
    "jquery": "^3.6.0",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.4",
    "mssql": "^6.3.2",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "performance-now": "^2.1.0",
    "puppeteer": "^13.3.2",
    "puppeteer-select": "^1.0.3",
    "reflect-metadata": "^0.1.13",
    "rest": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.8.1",
    "tslib": "^2.3.1",
    "typeorm": "^0.2.37",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "@types/rest": "^1.3.31",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-plugin-extra-rules": "^0.0.0-development",
    "luxon": "^3.0.1",
    "supertest": "^6.2.3"
  }
}
